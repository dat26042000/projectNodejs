extend ./layout/layout.pug
block content
	main.page-main
		.grid.wide.content-container
			//- Box on top
			.row.container.content-box-on-top
				.col.l-9.m-12.c-12.box-on-top__slider
					//- Slider banner
					.slider__banner
						.slider__banner-wrapper.fade
							img.slider__banner-img(src='./img/slider/armania_market1_03-1_slider.jpg', alt='')
							.slider__banner-text
								h3.slider__banner-text__title.text-1
								h3.slider__banner-text__title.text-2
								.slider__banner-text__msg
								.slider__banner-text__price.text-4
						.slider__banner-wrapper.fade
							img.slider__banner-img(src='./img/slider/armania_market1_03-2_slider.jpg', alt='')
							.slider__banner-text
								h3.slider__banner-text__title.text-1
								h3.slider__banner-text__title.text-2
								.slider__banner-text__msg
								.slider__banner-text__price.text-4
					.grid.slider__product-tab
						ul.row.slider__product-tab__list
							each data in randomProducts
								li.col.l-3.m-6.c-12.slider__product-tab__item.product-wrapper
									.product-tab__info
										a.product-tab__info-img.product-img(href='/product/'+ data._id)
											img(src='/uploads/images/' + data.images)
										span.group-product__sale.onsale
											if data.discount > 0
												span=data.discount
												| %
										.product-tab__info-detail
											a.product-tab__info-detail__name.product-name(href='/product/'+ data._id)=data.name 
											.product-tab__info-detail__price
												.product-price
													.price-final
														span=new Intl.NumberFormat('vn-IN', { maximumSignificantDigits: 3 }).format(((data.price)-(data.price*data.discount)/100).toFixed())
													.price-old
														span=new Intl.NumberFormat('vn-IN', { maximumSignificantDig: 3}).format((data.price).toFixed())
				.col.l-3.m-12.c-12.box-on-top__product-on-left
					.product-on-left__wrapper
						.product-on-left__bestseller
							| Bestseller Products
						.row.product-on-left__info-row.product-wrapper
							each data in bestSales
								.col.l-12.m-4.c-6.product-on-left__info
									a.product-on-left__info-img.product-img(href='/product/'+ data._id)
										img(src='/uploads/images/' + data.images)
									.product-on-left__info-detail.product-name
										a.product-on-left__name.product-name(href='/product/' + data._id)
											span=data.name
										span.group-product__sale.onsale
											if data.discount > 0
												span=data.discount
												| %
										.product-on-left__price
											.product-price
												.price-final
													span=new Intl.NumberFormat('vn-IN', { maximumSignificantDigits: 3 }).format(((data.price)-(data.price*data.discount)/100).toFixed())
												.price-old
													span=new Intl.NumberFormat('vn-IN', { maximumSignificantDig: 3}).format((data.price).toFixed())
							.product-on-left-action.pre-and-next
								.product-on-left-action__pre.product-action--pre
									i.previous-btn.product-on-left-action__pre-btn.ti-angle-left
								.product-on-left-action__next.show.product-action--next
									i.next-btn.product-on-left-action__next-btn.ti-angle-right
							div(style='text-align:center; width: 100%; height: 0;')
								span.dot
								span.dot
								span.dot
						.product-on-left__images
							.row.product-on-left__images-row
								each data in bestSales
									a.col.l-4.m-4.c-4.product-on-left__images-link(href='/product/' + data._id)
										img(src='/uploads/images/' + data.images)
			//- Hot deal
			.content-hot-deal.container
				.hot-deal__heading
					h2.hot-deal__heading__title Daily Deals
					.hot-deal__heading__countdown
						.countdown-days.countdown
							span.countdown-days__number.countdown-number
							span.countdown-days__text.countdown-text Days
						.countdown-hours.countdown
							span.countdown-hours__number.countdown-number 01
							span.countdown-hours__text.countdown-text Hours
						.countdown-mins.countdown
							span.countdown-mins__number.countdown-number 22
							span.countdown-mins__text.countdown-text Mins
						.countdown-secs.countdown
							span.countdown-secs__number.countdown-number 00
							span.countdown-secs__text.countdown-text Secs
					p.hot-deal__heading__des
						| Get Upto
						span 40%
						| Back On Purchase
					a.hot-deal__heading__view-btn(href='/shop') Shop Now
				.row.hot-deal__product
					each data in productsSale36
						.col.l-4.m-6.c-12.hot-deal__product-wrapper
							a.hot-deal__product-img(href='/product/' + data._id)
								img(src='/uploads/images/' + data.images)
							.hot-deal__product-detail
								a.hot-deal__product-detail__name.product-name(href='/product/' + data._id)=data.name
								.hot-deal__product-detail__price
									.product-price
										.price-final=new Intl.NumberFormat('vn-IN', { maximumSignificantDigits: 3 }).format(((data.price)-(data.price*data.discount)/100).toFixed())
										if data.discount > 0
											.price-old=new Intl.NumberFormat('vn-IN', { maximumSignificantDig: 3}).format((data.price).toFixed())
								.hot-deal__product-detail__des
									ul
										li Small screen 7.9&quot; Retina
										li Compatible with Apple Pencil
								span.hot-deal__product-detail__sale
									span.hot-deal__sale-title SALES
									span.hot-deal__sale-title-text=data.discount
									span.hot-deal__sale-title-text % OFF
								if data.amount > 0
									.hot-deal__product-detail__action
										a(href="/add-to-cart/" + data._id).hot-deal__product-detail__btn.btn ADD TO CART
										.hot-deal__product-detail__link.product-action
											a.quick-view
												i.far.fa-eye
											a.add-to-wish-list
												i.far.fa-heart
											a.to-compare(href='', title='Add to Compare')
												i.fas.fa-exchange-alt
								else
